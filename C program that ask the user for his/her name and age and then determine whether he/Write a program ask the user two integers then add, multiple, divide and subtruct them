#include <stdio.h>
int main()
{
    int n1, n2;
    int sum, subtract, multiply;
    float division;


    printf("Input any two integer: ");
    scanf("%d,%d", &n1, &n2);



    sum = n1 + n2;
    subtract = n1 - n2;
    multiply = n1 * n2;
    division = (float)n1 / n2;



    printf("ADD: %d\n", sum);
    printf("SUBTRACT: %d\n", subtract);
    printf("MULTIPLY: %d\n", multiply);
    printf("DIVISION: %f\n", division);


    return 0;
}
